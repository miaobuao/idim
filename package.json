{
  "name": "idim",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.15.3",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "dotenv -- turbo build",
    "dev": "dotenv -e .env.devel -- turbo dev",
    "lint": "turbo lint",
    "lint-staged": "turbo lint-staged",
    "type-check": "turbo type-check",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky",
    "db:migrate": "cd ./packages/db && pnpm prisma migrate",
    "db:studio": "cd ./packages/db && pnpm prisma studio",
    "db:generate": "cd ./packages/db && pnpm prisma generate"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "sass": "^1.71.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.7.0",
    "@repo/typescript-config": "workspace:*",
    "@types/lodash-es": "^4.17.12",
    "@unocss/eslint-plugin": "^0.58.5",
    "dotenv-cli": "latest",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "turbo": "latest"
  }
}
